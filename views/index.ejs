<!DOCTYPE html>
<html lang="en">
  <%- include('./partials/head.ejs') %>
  <body>
    <div class="container content">
      <%- include("./partials/header.ejs") %>
      <main>
        <% messages.forEach(message => { %>
        <div class="message">
          <h2><%= message.text %></h2>
          <p><%= message.user %></p>
          <p><%= message.added %></p>
        </div>
        <% }) %>
      </main>
      <%- include('./partials/footer.ejs') %>
    </div>
  </body>
</html>
